{
  "name": "debounce-decorator",
  "description": "Decorator component for debouncing class methods",
  "author": "Brian Vaughn",
  "user": "bvaughn",
  "version": "1.0.5",
  "scripts": {
    "test": "karma start",
    "tdd": "karma start --auto-watch --no-single-run",
    "dist": "babel ./source --out-dir ./dist-modules",
    "lint": "standard",
    "preversion": "npm run test && npm run lint && npm run dist && npm run dist-min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist"
  },
  "main": "dist",
  "dependencies": {},
  "files": [
    "dist"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "karma.conf.js",
      "webpack.config.babel.js",
      "vendors"
    ],
    "global": [
      "__DEVTOOLS__",
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "cancelAnimationFrame",
      "describe",
      "it",
      "requestAnimationFrame"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard"
    ],
    "globals": {
      "describe": false,
      "it": false
    }
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.4.0",
    "git-prepush-hook": "^1.0.1",
    "highlight.js": "^8.8.0",
    "html-webpack-plugin": "^1.6.2",
    "isparta-instrumenter-loader": "^0.2.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "link-package": "^0.1.0",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "purecss": "^0.6.0",
    "raw-loader": "^0.5.1",
    "standard": "^5.3.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-merge": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvaughn/debounce-decorator.git"
  },
  "homepage": "https://github.com/bvaughn/debounce-decorator/",
  "bugs": {
    "url": "https://github.com/bvaughn/debounce-decorator/issues"
  },
  "keywords": [
    "javascript",
    "decorator",
    "debounce",
    "deboucned",
    "es2015",
    "es6"
  ],
  "license": "MIT",
  "pre-push": []
}
